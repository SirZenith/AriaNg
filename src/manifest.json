{
    "background_color": "#000",
    "categories": [
        "utilities"
    ],
    "description": "AriaNg, a modern web frontend making aria2 easier to use. ",
    "display": "standalone",
    "file_handlers": [
        {
            "action": ".",
            "accept": {
                "application/x-bittorrent": [
                    ".torrent"
                ],
                "text/magnet": [
                    ".magnet"
                ]
            }
        }
    ],
    "icons": [
        {
            "src": "favicon.svg",
            "sizes": "any"
        },
        {
            "src": "pwa-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "pwa-512x512.png",
            "sizes": "512x512",
            "type": "image/png"
        },
        {
            "src": "pwa-512x512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any maskable"
        }
    ],
    "lang": "en",
    "launch_handler": {
        "client_mode": "focus-existing"
    },
    "name": "AriaNg",
    "protocol_handlers": [
        {
            "protocol": "magnet",
            "url": "/#!/new?uri=%s"
        }
    ],
    "scope": "./",
    "screenshots": [],
    "short_name": "AriaNg",
    "shortcuts": [
        {
            "name": "Dashboard",
            "url": "#!/downloading"
        },
        {
            "name": "Settings",
            "url": "#!/settings/ariang"
        },
        {
            "name": "Status",
            "url": "#!/status"
        }
    ],
    "start_url": ".",
    "theme_color": "#7abfd5"
}
